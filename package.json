{
  "name": "handbrake-queue",
  "version": "1.6.0",
  "description": "A utility to create queue files for Handbrake encoder.",
  "license": "MIT",
  "main": "electron/dist/main.js",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production --base-href ./",
    "watch": "ng build --watch --configuration development --base-href ./",
    "electron-build": "tsc --project electron/tsconfig.json",
    "electron-start": "concurrently \"npm run watch\" \"npm run electron-build && electron-forge start\"",
    "electron-package": "npm run build && npm run electron-build && electron-forge package",
    "electron-make": "npm run build && npm run electron-build && electron-forge make",
    "lint": "ng lint"
  },
  "repository": "https://github.com/M0ns1gn0r/HandbrakeQueue",
  "author": "Monsignor",
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.0.0",
    "@angular/common": "^19.0.0",
    "@angular/compiler": "^19.0.0",
    "@angular/core": "^19.0.0",
    "@angular/forms": "^19.0.0",
    "@angular/platform-browser": "^19.0.0",
    "@angular/platform-browser-dynamic": "^19.0.0",
    "@angular/router": "^19.0.0",
    "@electron/remote": "^2.1.2",
    "electron-squirrel-startup": "^1.0.1",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1",
    "zone.js": "^0.15.0",
    "video.js": "^8.19.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.0",
    "@angular-eslint/builder": "^19.0.2",
    "@angular-eslint/eslint-plugin": "^19.0.2",
    "@angular-eslint/eslint-plugin-template": "^19.0.2",
    "@angular-eslint/schematics": "^19.0.2",
    "@angular-eslint/template-parser": "^19.0.2",
    "@angular/cli": "^19.0.0",
    "@angular/compiler-cli": "^19.0.0",
    "@electron-forge/cli": "^7.5.0",
    "@electron-forge/maker-squirrel": "^7.5.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.5.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
    "electron": "^33.2.0",
    "eslint": "^9.15.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^17.13.2",
    "eslint-plugin-promise": "^7.1.0",
    "postcss": "^8.4.49",
    "ts-node": "^10.9.2"
  }
}
